{
  "name": "artwithaliens",
  "version": "1.0.0",
  "description": "A WordPress decoupled application in React built with Next.js",
  "scripts": {
    "analyze:bundle": "ANALYZE_BUNDLE=true yarn build",
    "analyze:unused": "next-unused",
    "build": "next build",
    "dev": "concurrently \"next dev\" \"yarn generate --watch\"",
    "format": "prettier --write \"{src,types}/**/*.{ts,tsx}\" *.{js,json,md,ts}",
    "generate": "graphql-codegen",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "start": "next start",
    "storybook": "start-storybook -s ./public"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "src"
    ],
    "exclude": [
      ".stories.",
      "src/service-worker.ts"
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artwithaliens/storefront.git"
  },
  "keywords": [
    "decoupled",
    "react",
    "next.js",
    "graphql"
  ],
  "author": "Eike Foken <eikefoken@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artwithaliens/storefront/issues"
  },
  "homepage": "https://artwithaliens.com",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@emotion/react": "^11.1.4",
    "@hookform/resolvers": "^1.2.0",
    "@material-ui/core": "^5.0.0-alpha.20",
    "@material-ui/lab": "^5.0.0-alpha.20",
    "@material-ui/styles": "^5.0.0-alpha.20",
    "@next/bundle-analyzer": "^10.0.4",
    "@next/plugin-sentry": "^10.0.4",
    "braintree-web": "^3.70.0",
    "clsx": "^1.1.1",
    "formik": "^2.2.6",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "html-react-parser": "^1.1.0",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.20",
    "next": "^10.0.4",
    "next-offline": "^5.0.3",
    "next-seo": "^4.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-cookie-consent": "^6.2.1",
    "react-dom": "^16.14.0",
    "react-ga": "^3.3.0",
    "react-hook-form": "^6.13.1",
    "react-is": "^16.13.1",
    "react-number-format": "^4.4.1",
    "react-swipeable-views": "^0.13.9",
    "react-use": "^15.3.4",
    "style-to-object": "^0.3.0",
    "type-fest": "^0.20.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@next/eslint-plugin-next": "^10.0.4",
    "@storybook/addon-a11y": "^6.1.11",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@types/braintree-web": "^3.69.0",
    "@types/lodash": "^4.14.166",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/workbox-sw": "^4.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "eslint-config-artwithaliens": "^1.8.0",
    "next-unused": "^0.0.3",
    "prettier": "^2.2.1",
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "shipit-shared": "^4.4.2",
    "typescript": "^4.1.3",
    "webpack": "^4.44.2"
  }
}
